			<Path
				Stroke="{StaticResource Compass_Indicator_InnerBorderBrush}"
				Margin="1"
				Stretch="Fill"
				StrokeMiterLimit="0"
				Data="M3.5,0.5 L26.5,0.5 C28.156855,0.49999988 29.5,1.8431089 29.5,3.4999181 L29.5,24.174082 C29.5,25.830891 28.156855,27.174 26.5,27.174 L11.549,27.174 11.549,29.454 0.5,29.454 0.5,24.174082 0.5,9.7429999 0.5,3.4999181 C0.5,1.8431089 1.8431457,0.49999988 3.5,0.5 z"/>
