	<!--Compass_IndicatorSideTemplate-->
	<ControlTemplate x:Key="Compass_IndicatorSideTemplate" TargetType="ContentControl">
		<Border
			x:Name="Background"
			BorderThickness="1"
			CornerRadius="{StaticResource Compass_Indicator_CornerRadius}"
			BorderBrush="{TemplateBinding BorderBrush}"
			Background="{TemplateBinding Background}">
			
			<Border
				x:Name="InnerBorder"
				BorderThickness="1"
				CornerRadius="{StaticResource Compass_Indicator_InnerCornerRadius}"
				BorderBrush="{StaticResource Compass_Indicator_InnerBorderBrush}">
				
				<ContentPresenter />
				
			</Border>
		</Border>
	</ControlTemplate>
	
